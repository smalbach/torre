    "@testing-library/user-event": "12.2.2",
    "axios": "^0.21.1",
    "bootstrap": "4.5.3",
    "chart.js": "2.9.4",
    "classnames": "2.2.6",
    "downshift": "^6.1.7",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "1.0.8",
    "moment": "2.29.1",
    "node-sass": "^6.0.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "perfect-scrollbar": "1.5.0",
    "react": "17.0.1",
    "react-accessible-dropdown-menu-hook": "^3.0.0",
